package javaprograms;

import java.util.ArrayList;
import java.util.HashMap;

public class Substrings {

	public static void main(String[] args) {
		
		String original = "Kushal is preparing a test for automation testing";
		String sub = "Test";
		ArrayList<String> subStrings = new ArrayList<String>();
		HashMap<String, ArrayList<Object>> output = new HashMap<String, ArrayList<Object>>();
		ArrayList<Integer> occurence = new ArrayList<Integer>();
		int count = 0;
		
		for(int i=0 ; i<sub.length(); i++) {
			for(int j=i+1 ; j<sub.length()+1; j++) {
				subStrings.add(sub.substring(i,j));
			}
		}
		
		System.out.println(subStrings);	
		
	//('t':[[1,2,3,4],8])	
				

	}

}
